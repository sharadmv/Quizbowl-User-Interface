<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel ui:field="wrapper">
		<g:ScrollPanel ui:field="scrollPanel">
			<g:VerticalPanel ui:field="chatPanel" />
		</g:ScrollPanel>
		<g:HorizontalPanel>
			<g:TextBox ui:field="inputBox" />
			<g:Button ui:field="sendButton">Send</g:Button>
		</g:HorizontalPanel>
		<g:Label ui:field="users" />
		<g:HTML ui:field="help" styleName="helpLink"><a>Help</a></g:HTML>
	</g:VerticalPanel>
</ui:UiBinder> 